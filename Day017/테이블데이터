CREATE TABLE cities (
  city_id INT PRIMARY KEY,
  city_name VARCHAR2(255),
  country_id INT,
  population INT
);

CREATE TABLE countries (
  country_id INT PRIMARY KEY,
  country_name VARCHAR2(255),
  continent VARCHAR2(255),
  population INT
);

